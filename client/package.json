{
  "name": "virgil-client",
  "version": "0.0.1",
  "description": "Poem sharing web client",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify -t babelify --extension=.jsx src/index.jsx -o public/app.js",
    "watch": "watchify -t babelify --extension=.jsx src/index.jsx -o public/app.js --debug --verbose",
    "lint": "./node_modules/eslint/bin/eslint.js src/*"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/michelmansour/virgil.git"
  },
  "dependencies": {
    "jquery": "3.1.0",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "react-router": "2.7.0",
    "react-sortablejs": "1.3.4",
    "sortablejs": "1.5.1"
  },
  "devDependencies": {
    "babel-eslint": "6.1.2",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babelify": "7.3.0",
    "eslint": "3.15.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  }
}
